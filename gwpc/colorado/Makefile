plss:
	cp ../../../plss/index.html ./plss
	cp ../../../plss/co.json ./plss
	cp ../../../plss/first.json ./plss
	cp ../../../plss/twnshp.json ./plss
	node app ../../../plss/map.js
	mv my.min.js ./plss/map.js
	node app ../../../plss/tile.js
	mv my.min.js ./plss/tile.js

