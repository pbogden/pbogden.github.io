<!DOCTYPE html>
<meta charset="utf-8">
<title>2018 bubble</title>
<link rel="stylesheet" type="text/css" href="./inspector.css">
<body>

<div id='container'>
    <div id='objectView' style="position: absolute; width:auto;">
    </div>
    <div id='chart'>
    </div>
</div>

<script type="module">

import {Runtime, Inspector} from "./runtime.js";
import define from "./@pbogden/2018-bubble.js";

new Runtime().module(define, name => {
  if (name === "chart") {
    return new Inspector(document.querySelector("#chart"));
  }
  if (name === "object") {
    return new Inspector(document.querySelector("#objectView"));
  }
});

